{
  "name": "react_SSR_redux_by_wabomba_bakar",
  "version": "1.0.0",
  "description": "A react redux server side rendered boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "node build/bundle.js",
    "build": "npm-run-all --parallel build:*",
    "app": "npm-run-all --parallel dev:*",
    "dev:watch": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:client": "webpack --config webpack.client.js --mode=development --watch",
    "dev:server": "webpack --config webpack.server.js --mode=development --watch",
    "dev:hot-reload": "webpack-dev-server --hotOnly",
    "build:client": "webpack --config webpack.config.js --mode=production --env.NODE_ENV=production",
    "build:server": "webpack --config webpack.server.js --mode=production --env.NODE_ENV=production"
  },
  "keywords": [
    "React",
    "redux",
    "Server Side Rendering with react"
  ],
  "author": "Wabomba Bakar",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/polyfill":"7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.0.1",
    "react-router-config": "^5.1.1",
    "sass-loader": "^8.0.0",
    "react-helmet": "^5.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^2.1.1",
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.26.3",
